actor calculator {
var cell: Int = 0;
public func sum(n: Int) : async Int {
cell += n;
cell
};
public func subtract(n: Int) : async Int {
cell -= n;
cell
};
public func multiply(n: Int) : async Int {
cell *= n;
cell;
};
public func divide(n: Int) : async ?Int {
if (n == 0) {
null;
}else {
cell /= n;
?cell 
};
};
public func clear() : async () {
cell := 0;
};
};
